{% extends "ZRRCandidatureBundle::layoutAdmin.html.twig" %}

{% block title %}
    Lecture d'un dossier - {{ parent() }}
{% endblock %}

{% block zrr_admin_body %}


    <h2 align="center"><strong>{{ 'dossier.numDossier'|trans }}</strong> <strong style="color: #ffcc00">{{ dossier.numDossier}}</strong></h2>
