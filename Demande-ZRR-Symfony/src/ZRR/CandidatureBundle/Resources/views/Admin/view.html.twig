                <td><strong>{{ 'dossier.avisResp'|trans }}</strong> {{ dossier.avisResp }}</td>
                <td><strong>{{ 'dossier.avisChef'|trans }}</strong> {{ dossier.avisChef}}</td>
            </tr>
