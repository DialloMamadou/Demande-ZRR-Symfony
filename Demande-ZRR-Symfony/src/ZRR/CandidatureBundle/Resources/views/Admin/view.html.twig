                    {{ dossier.zrr.laboratoire.dirigerLabos.responsableLabo.telLabo }}</td>
                <td><strong>{{ 'dossier.zrr.laboratoire.dirigerLabos.responsableLabo.emailLabo'|trans }} </strong>
                    {{ dossier.zrr.laboratoire.dirigerLabos.responsableLabo.emailLabo }}</td>
            </tr>
            #}
